<br>\r\n        <h2><strong>Galerija slika</strong></h2>\r\n        <br>\r\n      </div>\r\n      <div id=\"wrapper\">\r\n      <div id=\"sculpture-gallery\" class=\"carousel slide\" data-bs-ride=\"carousel\">\r\n        <div class=\"carousel-indicators\">\r\n          <button\r\n            type=\"button\"\r\n            data-bs-target=\"#sculpture-gallery\"\r\n            data-bs-slide-to=\"0\"\r\n            class=\"active\"\r\n          ></button>\r\n          <button\r\n            type=\"button\"\r\n            data-bs-target=\"#sculpture-gallery\"\r\n            data-bs-slide-to=\"1\"\r\n          ></button>\r\n          <button\r\n            type=\"button\"\r\n            data-bs-target=\"#sculpture-gallery\"\r\n            data-bs-slide-to=\"2\"\r\n          ></button>\r\n        </div>\r\n        <div class=\"carousel-inner\">\r\n          <div class=\"carousel-item active\">\r\n            <img\r\n              class=\"gallery\"\r\n              v-bind:src=\"'/artworks/' + sculpture.photo + '-1.jpg'\"\r\n              alt=\"\"\r\n            />\r\n          </div>\r\n          <div class=\"carousel-item\">\r\n            <img\r\n              class=\"gallery\"\r\n              v-bind:src=\"'/artworks/' + sculpture.photo + '-2.jpg'\"\r\n              alt=\"\"\r\n            />\r\n          </div>\r\n          <div class=\"carousel-item\">\r\n            <img\r\n              class=\"gallery\"\r\n              v-bind:src=\"'/artworks/' + sculpture.photo + '-3.jpg'\"\r\n              alt=\"\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <button\r\n          class=\"carousel-control-prev\"\r\n          type=\"button\"\r\n          data-bs-target=\"#sculpture-gallery\"\r\n          data-bs-slide=\"prev\"\r\n        >\r\n          <span class=\"carousel-control-prev-icon\"></span>\r\n        </button>\r\n        <button\r\n          class=\"carousel-control-next\"\r\n          type=\"button\"\r\n          data-bs-target=\"#sculpture-galle