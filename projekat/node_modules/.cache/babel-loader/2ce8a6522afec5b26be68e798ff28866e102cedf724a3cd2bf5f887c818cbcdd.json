{"ast":null,"code":"import $ from \"jquery\";\nimport NewestOffersView from \"../components/NewestOffersView.vue\";\nimport BannerHomeView from '../components/BannerHomeView.vue';\nimport NewestArtworkComponent from '../components/NewestArtworkComponent.vue';\nexport default {\n  components: {\n    NewestOffersView,\n    BannerHomeView,\n    NewestArtworkComponent\n  },\n  name: \"HomeView\",\n  mounted() {\n    if (this.verify() == \"serbian\") {\n      $(document).prop(\"title\", \"Fine Art Co Početna\");\n    } else {\n      $(document).prop(\"title\", \"Fine Art Co Home\");\n    }\n  },\n  methods: {\n    verify() {\n      return JSON.parse(localStorage.getItem(\"language\"));\n    }\n  }\n};","map":{"version":3,"names":["$","NewestOffersView","BannerHomeView","NewestArtworkComponent","components","name","mounted","verify","document","prop","methods","JSON","parse","localStorage","getItem"],"sources":["C:\\Users\\milic\\OneDrive\\Desktop\\FINALNO\\vd_projekat\\projekat\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"verify() == 'serbian'\" id=\"home\" class=\"container-fluid\">\r\n    <div class=\"row\">\r\n      <div class=\"col-sm-12\" id=\"title\">\r\n        <p>\r\n          Fine Art Co pruža posetiocima mogućnost da istraže i saznaju više o\r\n          umetničkim delima iz kolekcije galerije, kao i da postave svoje ponude\r\n          i komentare. Korisnici mogu pregledati slike, skulpture i ostale\r\n          umetnine i saznati više o autorima i istoriji umetnosti iz perioda\r\n          renesanse.\r\n        </p>\r\n      </div>\r\n    </div>\r\n    <NewestArtworkComponent></NewestArtworkComponent>\r\n    <NewestOffersView></NewestOffersView>\r\n    <banner-home-view></banner-home-view>\r\n  </div>\r\n  <div v-else id=\"home\" class=\"container-fluid\">\r\n    <div class=\"row\">\r\n      <div class=\"col-sm-12\" id=\"title\">\r\n        <p>\r\n          Fine Art Co features a wide variety of art styles, including\r\n         