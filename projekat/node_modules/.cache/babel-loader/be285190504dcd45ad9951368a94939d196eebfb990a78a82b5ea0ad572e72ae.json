{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport $ from \"jquery\";\nimport sculptures from \"../data/sculptures.js\";\nexport default {\n  name: \"SculptureView\",\n  data() {\n    return {\n      sculptures: sculptures,\n      sculpture: {},\n      offers: [],\n      user: \"\"\n    };\n  },\n  created() {\n    let sculptureId = Number(this.$route.params.id);\n    this.sculpture = this.sculptures.find(sculpture => sculpture.id == sculptureId);\n    this.user = localStorage.getItem(\"user\");\n    this.offers = JSON.parse(localStorage.getItem(\"offers\"));\n    let sculpture = this.sculpture;\n    this.offers = this.offers.filter(function (elem) {\n      return elem.artwork == sculpture.name;\n    });\n    this.messages = JSON.parse(localStorage.getItem(\"messages\"));\n    this.messages = this.messages.filter(function (elem) {\n      return elem.artist == sculpture.artist;\n    });\n    if (this.verify() == \"serbian\") {\n      $(document).prop(\"title\", \"Fine Art Co Skulpture\");\n    } else {\n      $(document).prop(\"title\", \"Fine Art Co Sculptures\");\n    }\n  },\n  methods: {\n    postOffer() {\n      if (localStorage.getItem(\"user\") == null) {\n        if (this.verify() == \"serbian\") {\n          alert(\"Niste ulogovani.\");\n        } else {\n          alert(\"You are not logged in.\");\n        }\n        return;\n      }\n      let id = 0;\n      let offers = JSON.parse(localStorage.getItem(\"offers\"));\n      for (let i = 0; i < offers.length; i++) {\n        if (offers[i].id > id) {\n          id = offers[i].id;\n        }\n      }\n      let newOffer = {\n        id: id + 1,\n        username: this.user,\n        artwork: this.sculpture.name,\n        bidding: this.bidding,\n        date: new Date()\n      };\n      offers.push(newOffer);\n      localStorage.setItem(\"offers\", JSON.stringify(offers));\n      this.offers = JSON.parse(localStorage.getItem(\"offers\"));\n      let sculpture = this.sculpture;\n      this.offers = this.offers.filter(function (elem) {\n        return elem.artwork == sculpture.name;\n      });\n    },\n    postMessage() {\n      if (localStorage.getItem(\"user\") == null) {\n        if (this.verify() == \"serbian\") {\n          alert(\"Niste ulogovani.\");\n        } else {\n          alert(\"You are not logged in.\");\n        }\n        return;\n      }\n      let id = 0;\n      let messages = JSON.parse(localStorage.getItem(\"messages\"));\n      for (let i = 0; i < messages.length; i++) {\n        if (messages[i].id > id) {\n          id = messages[i].id;\n        }\n      }\n      let newMessage = {\n        id: id + 1,\n        username: this.user,\n        idArtist: this.sculpture.idArtist,\n        artist: this.sculpture.artist,\n        text: this.message\n      };\n      messages.push(newMessage);\n      localStorage.setItem(\"messages\", JSON.stringify(messages));\n      this.messages = JSON.parse(localStorage.getItem(\"messages\"));\n      let sculpture = this.sculpture;\n      this.messages = this.messages.filter(function (elem) {\n        return elem.artist == sculpture.artist;\n      });\n    },\n    verify() {\n      return JSON.parse(localStorage.getItem('language'));\n    }\n  }\n};","map":{"version":3,"names":["$","sculptures","name","data","sculpture","offers","user","created","sculptureId","Number","$route","params","id","find","localStorage","getItem","JSON","parse","filter","elem","artwork","messages","artist","verify","document","prop","methods","postOffer","alert","i","length","newOffer","username","bidding","date","Date","push","setItem","stringify","postMessage","newMessage","idArtist","text","message"],"sources":["C:\\Users\\zeljk\\Desktop\\FAKS\\3. godina\\VD\\projekat\\vd_projekat\\projekat\\src\\views\\SculptureView.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"verify() == 'serbian'\" class=\"container-fluid\" id=\"sculpture-container\">\r\n    <div class=\"row\">\r\n     <div class=\"col-sm-12\" style=\"float:le