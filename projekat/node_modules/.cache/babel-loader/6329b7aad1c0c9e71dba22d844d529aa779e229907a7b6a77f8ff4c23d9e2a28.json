n: none; color: inherit\"\r\n          :to=\"/paintings/ + p.id\"\r\n        >\r\n          <div\r\n            class=\"card border-0\"\r\n            style=\"\r\n              background-color: rgba(255, 255, 255, 0.8);\r\n              margin: auto;\r\n              height: 100%;\r\n              margin-top: 30px;\r\n            \"\r\n          >\r\n            <img\r\n              class=\"card-img\"\r\n              v-bind:src=\"'artworks/' + p.photo + '.jpg'\"\r\n              alt=\"image\"\r\n              style=\"\r\n                min-height: 280px;\r\n                height: 280px;\r\n                margin-top: 10px;\r\n                margin-left: 10px;\r\n                margin-right: 10px;\r\n              \"\r\n            />\r\n            <div class=\"card-body d-flex flex-column\">\r\n              <h5 class=\"card-title\">{{ p.ime }}</h5>\r\n              <p class=\"card-text\">\r\n                {{ p.opis }}\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </router-link>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div v-else class=\"container-fluid\" id=\"paintings\">\r\n    <div class=\"row\" style=\"display: flex; justify-content: right\">\r\n      <div class=\"col-sm-12 nput-group mt-2\" style=\"margin-top: 20px\">\r\n        <router-link to=\"/\" style=\"text-decoration: none; color: gray\">\r\n          Home\r\n        </router-link>\r\n        / <span style=\"color: gray\"> Artworks </span> / Paintings\r\n      </div>\r\n      <div class=\"col-sm-12\" style=\"margin-top: 20px\">\r\n        <div class=\"form-outline w-auto\">\r\n          <input\r\n            type=\"search\"\r\n            id=\"form1\"\r\n            class=\"form-control-dropdown\"\r\n            v-model=\"searchWord\"\r\n          />\r\n          &nbsp;\r\n          <button class=\"btn btn-dark\" for=\"form1\" @click=\"search()\">\r\n            Search\r\n          </button>\r\n          <div class=\"dropdown\">\r\n            <button\r\n    