rs[others.length - 1];\n    this.artworks[2].link = \"/others/\" + others[others.length - 1].id;\n  },\n  methods: {\n    verify() {\n      return JSON.parse(localStorage.getItem('language'));\n    }\n  }\n};","map":{"version":3,"names":["name","data","offers","artworks","created","JSON","parse","localStorage","getItem","sort","a","b","Date","date","paintings","sculptures","others","splice","i","length","painting","find","elem","artwork","sculpture","other","photo","link","id","day","getDate","month","getMonth","year","getYear","methods","verify"],"sources":["C:\\Users\\milic\\OneDrive\\Desktop\\FINALNO\\vd_projekat\\projekat\\src\\components\\NewestArtworkComponent.vue"],"sourcesContent":["<template>\r\n  <section v-if=\"verify() == 'serbian'\" class=\"artworks-area section-gap\" id=\"artworks\">\r\n    <div class=\"container\" id=\"newest-artworks\">\r\n      <div class=\"row d-flex justify-content-center\">\r\n        <div class=\"menu-content pb-60 col-lg-10\">\r\n          <h1 class=\"mb-10\" id=\"artworks-title\">Najnovije umetnine</h1>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"col-md-4\" v-for=\"a of artworks\" :key=\"a.id\" style=\"margin-top: 20px\">\r\n          <router-link :to=\"o.link\" style=\"text-decoration: none\">\r\n            <div class=\"card\">\r\n              <img\r\n                class=\"card-img-top\"\r\n                v-bind:src=\"'artworks/' + a.photo + '.jpg'\"\r\n                alt=\"\"\r\n                style=\"min-height: 200px; height: 200px\"\r\n              />\r\n              <div class=\"card-body\">\r\n                <h4>{{ a.ime }}</h4>\r\n                <p>\r\n                  <b>Umetnik</b>: {{ a.umetnik }}<br />\r\n                  <b>Vrednost</b>: {{ a.vrednost }}\r\n                </p>\r\n              </div>\r\n            </div>\r\n            </router-link>\r\n        </div>\r\n      </div>\r\n      <div class=\"row\">\r\n        <br />\r\n      </div>\r\n    